{
	"mcpServers": {
		"Astro": {
			"type": "streamable-http",
			"url": "https://mcp.docs.astro.build/mcp",
			"alwaysAllow": ["search_astro_docs"]
		},
		"Svelte": {
			"type": "streamable-http",
